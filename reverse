import java.util.Collections;

public class map {
    public static void main(String[] args) {
        String s="Dani waniy";
        String output="";
        String[] words=s.split(" ");
        for(String word : words){
                String rev="";
                for(int i=word.length()-1;i>=0;i--){
                    rev += word.charAt(i);
                }

                output = output + rev +" ";

            }
          System.out.println(output);

        }

     }
